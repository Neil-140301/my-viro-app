{
	"expo": {
		"name": "my-viro-app",
		"slug": "my-viro-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.neil_140301.myviroapp.dev"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": ["@viro-community/react-viro", "expo-camera"]
	}
}
